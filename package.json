{
  "name": "serverless-log-dumpster",
  "version": "0.1.0",
  "description": "Serverless Framework plugin to archive AWS CloudWatch log groups before deletion",
  "main": "src/index.js",
  "repository": "git@github.com:xoeye/serverless-log-dumpster.git",
  "author": "Xavier Bergeron <xavbergeron@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "pre-commit": "lint-staged",
    "lint": "eslint src",
    "test": "mocha",
    "test:coverage": "nyc --reporter=lcov mocha",
    "prepare": "husky install",
    "demo:create": "cd demo-projects/create-log-group && serverless deploy",
    "demo:delete": "cd demo-projects/delete-log-group && serverless deploy",
    "demo:teardown": "cd demo-projects/create-log-group && serverless deploy",
    "demo": "yarn demo:create && yarn demo:delete; yarn demo:teardown"
  },
  "dependencies": {
    "@aws-cdk/cloudformation-diff": "^1.124.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "prettier-eslint-cli": "^5.0.1",
    "serverless": "^2.60.1",
    "sinon": "^11.1.2"
  },
  "peerDependencies": {
    "serverless": ">= 1.15.0 <3"
  },
  "engines": {
    "node": ">=12.0"
  }
}
